original = input('Type an unstevenified phrase: ')
original = original.lower()
removed = original.replace("r","")
removed = removed.replace("t","")
removed = removed.replace("f","")
removed = removed.replace("g","")
removed = removed.replace("v","")
removed = removed.replace("b","")
removed = removed.replace("4","")
removed = removed.replace("5","")
print (removed)
